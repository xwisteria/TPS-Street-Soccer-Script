for i,BALLFIRE in pairs(Ball:GetChildren()) do
    if BALLFIRE:IsA("Fire") then
       BALLFIRE.Heat = 1
       BALLFIRE.Size = 5
    end
end

Ball.Massless = true
Ball.CustomPhysicalProperties = false
                        end
