        Callback = function(Value)
            b2b_REACH_X, b2b_REACH_Y, b2b_REACH_Z = Value, Value, Value
            b2b_reachIndicator.Size = Vector3.new(b2b_REACH_X * 2, b2b_REACH_Y * 2, b2b_REACH_Z * 2)
        end
    })

