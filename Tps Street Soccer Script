Event:FireServer(A_1, A_2)

local Events3 = game:GetService("Workspace").FE.Kick.RemoteEvent2
Events3:FireServer()
end
end
end
end
end)
    else
if TouchedGKBallBox then
TouchedGKBallBox:Disconnect()
end
end
    end)

